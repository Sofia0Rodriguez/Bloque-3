def tienen_elemento_comun(lista1, lista2):
    # Comprobamos si hay algún elemento común entre las dos listas usando set
    return bool(set(lista1) & set(lista2))

# Listas de ejemplo
lista1 = [1, 2, 9, 1]
lista2 = [4, 5, 6, 7]

# Llamamos a la función y mostramos el resultado
resultado = tienen_elemento_comun(lista1, lista2)
print(resultado)  # Esto imprimirá: True

